.page {
  width: 100%;
  height: 100%;
  padding-top: 20px;
}

.item {
  height: 80px;
  margin-left: 20px;
  border-bottom: 1px solid #f2f2f2;
  width: 90%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.item image {
  width: 14px;
  height: 14px;
  margin-right: 5px;
}

.item-right-image {
  position: absolute;
  right: 10%;
}